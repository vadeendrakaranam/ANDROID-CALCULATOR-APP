<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/black"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="38dp"
        android:layout_marginBottom="598dp"
        android:text="CALCULATOR"
        android:textColor="#F9A825"
        android:textSize="50dp"
        android:textStyle="bold"
        tools:ignore="HardcodedText,SpUsage" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="362dp"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="9dp"
        android:layout_marginBottom="28dp"
        android:text="DEVELOPER-VADEENDRA"
        android:textColor="@color/white"
        android:textSize="30dp"
        tools:ignore="HardcodedText,SpUsage" />

    <Button
        android:id="@+id/add"
        android:layout_width="152dp"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="201dp"
        android:layout_marginBottom="287dp"
        android:backgroundTint="#9E9D24"
        android:onClick="onAdd"
        android:text="ADD"
        tools:ignore="HardcodedText,TextContrastCheck" />

    <Button
        android:id="@+id/div"
        android:layout_width="163dp"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="29dp"
        android:layout_marginBottom="217dp"
        android:backgroundTint="#EF6C00"
        android:text="DIV"
        android:onClick="onDiv"
        tools:ignore="HardcodedText,TextContrastCheck" />

    <Button
        android:id="@+id/mul"
        android:layout_width="152dp"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="201dp"
        android:layout_marginBottom="219dp"
        android:backgroundTint="#00838F"
        android:text="MUL"
        android:onClick="onMul"
        tools:ignore="HardcodedText" />

    <Button
        android:id="@+id/sub"
        android:layout_width="155dp"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="35dp"
        android:layout_marginBottom="285dp"
        android:backgroundTint="#558B2F"
        android:text="SUB"
        android:onClick="onSub"
        tools:ignore="HardcodedText,TextContrastCheck" />

    <TextView
        android:layout_width="248dp"
        android:layout_height="44dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="148dp"
        android:layout_marginBottom="526dp"
        android:background="#9E9D24"
        android:text="ENTER 1ST NUMBER :"
        android:textColor="@color/white"
        android:textSize="24sp"
        android:textStyle="bold"
        tools:ignore="HardcodedText,TextContrastCheck,TextSizeCheck" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="248dp"
        android:layout_height="44dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="149dp"
        android:layout_marginBottom="459dp"
        android:background="#00838F"
        android:text="ENTER 2ND NUMBER :"
        android:textColor="@color/white"
        android:textSize="24sp"
        android:textStyle="bold"
        tools:ignore="HardcodedText,TextSizeCheck" />

    <EditText
        android:id="@+id/num1"
        android:layout_width="132dp"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="5dp"
        android:layout_marginBottom="527dp"
        android:background="@color/white"
        android:ems="10"
        android:inputType="number"
        android:textSize="40dp"
        tools:ignore="Autofill,LabelFor,SpUsage,SpeakableTextPresentCheck,TouchTargetSizeCheck" />

    <EditText
        android:id="@+id/num2"
        android:layout_width="132dp"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="4dp"
        android:layout_marginBottom="453dp"
        android:background="@color/white"
        android:ems="10"
        android:inputType="number"
        android:textSize="40dp"
        tools:ignore="Autofill,LabelFor,SpUsage,SpeakableTextPresentCheck,TouchTargetSizeCheck" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="216dp"
        android:layout_marginBottom="117dp"
        android:background="#C62828"
        android:text="RESULT :"
        android:textColor="@color/white"
        android:textSize="30dp"
        android:textStyle="bold"
        tools:ignore="HardcodedText,SpUsage,TextSizeCheck" />

    <TextView
        android:id="@+id/res"
        android:layout_width="160dp"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="43dp"
        android:layout_marginBottom="100dp"
        android:background="@color/white"
        android:ems="10"
        android:freezesText="true"
        android:inputType="number"
        android:text=""
        android:textColor="@color/black"
        android:textSize="50dp"
        android:textStyle="bold"
        tools:ignore="Autofill,LabelFor,SpUsage,SpeakableTextPresentCheck,TextViewEdits" />
</RelativeLayout>